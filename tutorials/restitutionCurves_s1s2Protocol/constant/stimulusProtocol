//Stimulus protocol for singleCell Models and for 3D voltage sources based on location/Cell ID

//stimulus constants

	stim_start  	20;	//(millisecond)
	stim_duration   1;	//(millisecond)

	//NOTE: introduce module of amplitude.
	//stim_amplitude  52;	//(picoA_per_picoF) (TNNP)
stim_amplitude   70.0;

//S1 protocol

stim_period_S1  2000;
nstim1  10;

//S2 protocol

stim_period_S2  250;
nstim2  2;

writeAfterTime 18;

