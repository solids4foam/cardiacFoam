//Conductivity. The 1D and 2D use only the respective dimension of the tensor.

conductivity [-1 -3 3 0 0 2 0] (0.1334177215 0 0 0.01760617761 0 0.01760617761); //3D

// Cellular membrane surface-to-volume ratio [1/m]

	chi  [0 -1 0 0 0 0 0] 140000;

// Cellular membrane capacitance [F/m^2]

	Cm    [-1 -4 4 0 0 2 0] 0.01;

// Specify the ionic model

ionicModel  Courtemanche;

//specify the type of tissue or dimension (for FDA solutions )to model

//	tissue myocyte;
tissue      myocyte;

