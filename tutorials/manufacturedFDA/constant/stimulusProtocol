FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      stimulusProtocol;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



//Stimulus is an initialization in the solver. Read 0 values from the dictionary.




// ************************************************************************* //



// External stimulus current location

stimulusLocationMin    (0 0 5.5e-3);
stimulusLocationMax    (1.5e-3 1.5e-3 7e-3);


// External stimulus current characteristics

stimulusDuration    [0 0 1 0 0 0 0] 0;

stimulusIntensity   [0 -3 0 0 0 1 0] 0;


//Read to run only
//stimulus constants

	stim_start  	0;	//(millisecond)
	stim_duration   0;	//(millisecond)
	stim_amplitude  0;	//(picoA_per_picoF)



//S1 protocol 

	stim_period_S1  0;
	nstim1  0;



//S2 protocol

	stim_period_S2  0;
	nstim2  0;

// ************************************************************************* //


