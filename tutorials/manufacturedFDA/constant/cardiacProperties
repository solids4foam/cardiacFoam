FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      cardiacProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//Conductivity. The 1D and 2D use only the respective dimension of the tensor. 

	conductivity [-1 -3 3 0 0 2 0] (0.111453302 0 0 0.121585420 0 0.030396355); //3D


// Cellular membrane surface-to-volume ratio [1/m]

	chi  [0 -1 0 0 0 0 0] 3;


// Cellular membrane capacitance [F/m^2]

	Cm    [-1 -4 4 0 0 2 0] 2;



// Specify the ionic model

	ionicModel tmanufacturedFDA;
		exportedVariables (u1 u2 u3);


//solution for the manufactured solution initialization and ionic model constants

	dimension "1D"; 
//	dimension "2D";
//	dimension "3D"; 
 


