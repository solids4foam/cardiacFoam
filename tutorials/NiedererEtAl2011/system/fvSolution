FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "Vm|VmFinal|u|uFinal"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-11;
        relTol          0.0;
    }

    "psi|psiFinal"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-11;
        relTol          0.1;
    }
}

// PIMPLE settings for eikonalElectroActivationFoam
PIMPLE
{
    nOuterCorrectors    1000;

    residualControl
    {
        "Vm|psi"
        {
            relTol          0.0;
            tolerance       1e-6;
        }
    }
}

relaxationFactors
{
    equations
    {
        psi    0.9;
    }
    fields
    {
        //psi    0.5;
    }
}

