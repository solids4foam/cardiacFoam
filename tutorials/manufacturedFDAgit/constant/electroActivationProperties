FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      electroActivationProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//Conductivity. The 1D and 2D use only the respective dimension of the tensor. 

conductivity [-1 -3 3 0 0 2 0] (0.111453302 0 0 0.121585420 0 0.030396355); //3D



// Cellular membrane surface-to-volume ratio [1/m]
Chi  [0 -1 0 0 0 0 0] 3;

// Cellular membrane capacitance [F/m^2]
Cm    [-1 -4 4 0 0 2 0] 2;

solveExplicit yes;


ionicModelCoeffs
{
    // Specify the ionic model
    //ionicModel newmanufacturedFDA;
    ionicModel manufacturedFDA;

    //specify the type of tissue to model
    dimension "1D"; 
    //dimension "2D";
    //dimension "3D";  


    //Initial ODE step;
    initialODEStep 1e-5;


    // ODE solver
    solver    RKF45;
    //solver    Euler;

    // Maximum number of steps
    maxSteps 1000000000;

    // Relative tolerance
    // Defaults to 1e-4
    //relTol 1e-6;

    // Absolute tolerance
    // Defaults to 1e-15
    //absTol 1e-6;

    
}

