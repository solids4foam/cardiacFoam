// ODE solver
solver    RKF45;
//solver    Euler;

// Maximum number of steps
maxSteps 1000000000;

// Relative tolerance
// Defaults to 1e-4
//relTol 1e-4;

// Absolute tolerance
// Defaults to 1e-15
absTol 1e-15;


