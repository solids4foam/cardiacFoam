FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      t;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Dimensionless thickness
dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    fixed
    {
        type            zeroGradient;
    }
    inside
    {
        type            fixedValue;
        value           uniform 0;
    }
    outside
    {
        type            fixedValue;
        value           uniform 1;
    }
}

