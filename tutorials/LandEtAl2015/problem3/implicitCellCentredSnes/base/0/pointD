FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    fixed
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    inside
    {
        type            calculated;
    }
    outside
    {
        type            calculated;
    }
}

