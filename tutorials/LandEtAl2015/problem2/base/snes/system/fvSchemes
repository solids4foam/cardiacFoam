FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default                 backward;
}

ddtSchemes
{
    default                 backward;
}

gradSchemes
{
    default                 leastSquaresS4f;
}

divSchemes
{
    default                 Gauss linear;
}

laplacianSchemes
{
    default                 none;
    laplacian(DD,D)         Gauss linear corrected;
}

snGradSchemes
{
    default                 corrected;
}

interpolationSchemes
{
    default                 linear;
}

