FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      stimulusProtocol;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// External stimulus current location

stimulusLocationMinList ( (0 0 0) (7e-3 0 0) );
stimulusLocationMaxList ( (1.0e-3 20.0e-3 0.1e-3) (10.0e-3 8.0e-3 0.1e-3) );
stimulusStartTimeList   ( 0.0 0.34 );

// External stimulus current characteristics

stimulusDuration    [0 0 1 0 0 0 0] 3e-3;

stimulusIntensity   [0 -3 0 0 0 1 0] 50000;

//Read to run only
//stimulus constants

	stim_start  	0;	//(millisecond)
	stim_duration   0;	//(millisecond)
	stim_amplitude  0;	//(picoA_per_picoF)

//S1 protocol

	stim_period_S1  0;
	nstim1  0;

//S2 protocol

	stim_period_S2  0;
	nstim2  0;

// ************************************************************************* //

/*
PS: Niederer Stimulus in VolField

 1. External stimulus current intensity [A/m^2]
 2. Per A/m^2 is calculated as A/m^3 * beta (above)

	stimulusIntensity   [0 -2 0 0 0 1 0] 0.000000357142857;
	External stimulus current intensity [A/m^3]
 	50000 micro A/cm3 == 50000 A/m3
*/

